! X colors.
! Generated by 'wal'
*foreground:        #ddeabf
*background:        #0F1E25
*.foreground:       #ddeabf
*.background:       #0F1E25
emacs*foreground:   #ddeabf
emacs*background:   #0F1E25
URxvt*foreground:   #ddeabf
XTerm*foreground:   #ddeabf
UXTerm*foreground:  #ddeabf
URxvt*background:   [100]#0F1E25
XTerm*background:   #0F1E25
UXTerm*background:  #0F1E25
URxvt*cursorColor:  #ddeabf
XTerm*cursorColor:  #ddeabf
UXTerm*cursorColor: #ddeabf
URxvt*borderColor:  [100]#0F1E25

! Colors 0-15.
*.color0: #0F1E25
*color0:  #0F1E25
*.color1: #3E8A60
*color1:  #3E8A60
*.color2: #54986D
*color2:  #54986D
*.color3: #5CA175
*color3:  #5CA175
*.color4: #8EB779
*color4:  #8EB779
*.color5: #77B188
*color5:  #77B188
*.color6: #A7CE88
*color6:  #A7CE88
*.color7: #ddeabf
*color7:  #ddeabf
*.color8: #9aa385
*color8:  #9aa385
*.color9: #3E8A60
*color9:  #3E8A60
*.color10: #54986D
*color10:  #54986D
*.color11: #5CA175
*color11:  #5CA175
*.color12: #8EB779
*color12:  #8EB779
*.color13: #77B188
*color13:  #77B188
*.color14: #A7CE88
*color14:  #A7CE88
*.color15: #ddeabf
*color15:  #ddeabf

! Black color that will not be affected by bold highlighting.
*.color66: #0F1E25
*color66:  #0F1E25

! Xclock colors.
XClock*foreground: #ddeabf
XClock*background: #0F1E25
XClock*majorColor:  rgba:dd/ea/bf/ff
XClock*minorColor:  rgba:dd/ea/bf/ff
XClock*hourColor:   rgba:dd/ea/bf/ff
XClock*minuteColor: rgba:dd/ea/bf/ff
XClock*secondColor: rgba:dd/ea/bf/ff

! Set depth to make transparency work.
URxvt*depth: 32
!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Fonts -- !
  URxvt.font:xft:Monospace-Bold:pixelsize=12.5
  URxvt.boldfont:xft:Monospace-Bold:pixelsize=12.5


URxvt.letterSpace: -1

URxvt.internalBorder:  10
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 2

! -- xterm config -- !


xterm*faceName: Monospace


xterm*faceSize: 10
xterm*renderFont: true
xterm*BorderWidth: 15
XTerm*selectToClipboard: true
