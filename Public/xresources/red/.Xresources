! Xclock colors.
XClock*foreground: #acbcd5
XClock*background: #0b0404
XClock*majorColor:  rgba:ac/bc/d5/ff
XClock*minorColor:  rgba:ac/bc/d5/ff
XClock*hourColor:   rgba:ac/bc/d5/ff
XClock*minuteColor: rgba:ac/bc/d5/ff
XClock*secondColor: rgba:ac/bc/d5/ff

! Set depth to make transparency work.
URxvt*depth: 32

! Set depth to make transparency work.
URxvt*depth: 32
!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Fonts -- !
  URxvt.font:xft:Monospace-Bold:pixelsize=12.5
  URxvt.boldfont:xft:Monospace-Bold:pixelsize=12.5


URxvt.letterSpace: -1

URxvt.internalBorder:  10
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 2

! -- xterm config -- !


xterm*faceName: Monospace


xterm*faceSize: 10
xterm*renderFont: true
xterm*BorderWidth: 10
XTerm*selectToClipboard: true
